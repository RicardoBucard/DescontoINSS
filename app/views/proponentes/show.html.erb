<div class="card text-white text-center p-3">
  <h1 class="title-card-text"><strong><%= @proponente.nome %></strong></h1>
</div>
<div class="card-container">
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>CPF: </strong> <span class="card-text" ><%= @proponente.cpf %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Data de Nascimento: </strong> <span class="card-text"><%= @proponente.data_nascimento %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Logradouro: </strong> <span class="card-text"><%= @proponente.logradouro %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Número: </strong> <span class="card-text"><%= @proponente.numero %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Bairro: </strong> <span class="card-text"><%= @proponente.bairro %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Cidade: </strong> <span class="card-text"><%= @proponente.cidade %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Estado: </strong> <span class="card-text"><%= @proponente.estado %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>CEP: </strong> <span class="card-text"><%= @proponente.cep %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Telefone Pessoal: </strong> <span class="card-text"><%= @proponente.telefone_pessoal %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Telefone de Referência: </strong> <span class="card-text"><%= @proponente.telefone_referencia %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Salário: </strong> <span id="salario" class="card-text">R$ <%= @proponente.salario %></span>
      </h5>
    </div>
  </div>
  <div class="card text-white text-center p-3">
    <div class="card-body">
      <h5 class="card-title">
        <strong>Desconto do INSS: </strong> <span class="card-text" id="proponente_desconto_inss" >R$ <%= @proponente.desconto_inss %></span>
      </h5>
    </div>
</div>
<div class="card-button-container">
  <%= link_to 'Editar', edit_proponente_path(@proponente) %> |
  <%= link_to 'Voltar', proponentes_path %> |
  <%= link_to 'Deletar', @proponente, method: :delete, data: { confirm: 'Tem certeza que deseja deletar?' }%>
</div>
